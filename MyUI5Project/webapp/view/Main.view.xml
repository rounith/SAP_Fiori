<core:View xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core.mvc" xmlns="sap.m" controllerName="cloud4c.fiori.controller.Main">
	<f:SimpleForm editable="true" id="simpleForm">
		<Label text="EmpID"></Label>
		<Input width="40%" id="idId" enabled="{/empStr/status}"></Input>
		<Label text="EmpName"></Label>
		<Input width="40%"  id="idName" value="{/empStr/empName}" enabled="{/empStr/status}"></Input>
		<Label text="Salary"></Label>
		<Input width="40%" id="idSalary" value="{iron>/empStr/salary}" enabled="{/empStr/status}" />
		<Label text="Currency"></Label>
		<Input width="40%" id="idCurr" value="{= ${/empStr/empName} === 'Vijay' ? 900 : 456}" enabled="{/empStr/status}"></Input>
        <Label></Label>
		<Button text="Click Me" width="40%" press="Magic"></Button>
		<Label></Label>
		<Button text="SampleJSON" width="40%" id="sample" press="Switch"></Button>
		<Button text="MulModelJSON" width="40%" id="mulModel" press="MulSwitch"></Button>
	</f:SimpleForm>
</core:View>

<!-- **** when you multiple models to bind data use > this with model name 
to get data from that model if not used then it will take default model
For example: {/empStr/empId} (This will take data from default model)
			 {iron>/empStr/empId} (This will take data from iron named model)-->